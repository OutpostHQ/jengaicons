{
  "name": "jenga-icons",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "turbo build",
    "build-packages": "pnpm -r --filter react-icons build",
    "preview": "vite preview",
    "env": "node src/setupEnv.ts",
    "fetch": "node src/index.ts",
    "generate-icons": "rm -rf ./icons && npx @svgr/cli --typescript -d icons assets",
    "test": "TZ=Asia/Singapore jest",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.7.1"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@svgr/cli": "^6.2.1",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "turbo": "^1.7.2",
    "typescript": "^4.6.3"
  }
}
