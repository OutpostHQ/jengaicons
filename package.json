{
  "name": "jenga-icons",
  "version": "0.0.0",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean && rm -rf node_modules",
    "generate-optimized": "pnpm --filter generate-optimized build && pnpm --filter generate-optimized write-icons ../../assets",
    "generate-react": "pnpm --filter generate-jengaicons-react write-icons --assets ../../optimized --root-path ../../packages/jengaicons-react --output-folder-name icons"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@svgr/cli": "^6.2.1",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "turbo": "^1.7.2",
    "typescript": "^4.6.3"
  }
}
